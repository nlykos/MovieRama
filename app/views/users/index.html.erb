<% provide(:title, 'All users') %>

<h1>Listing Users</h1>
<% @users.each do |user| %>
  <dl class="dl-horizontal results">
  <h3>
    <dt>Ονομα: </dt><dd><%= user.name %></dd>
    <dt>Username: </dt><dd><%= user.username %></dd>
    <dt>Email: </dt><dd><%= user.email %><dd>
  </h3>
  </dl>
<% end %>
